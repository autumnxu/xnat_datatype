#macro(showBoolean $property $item)
<!-- BEGIN Generated by the showBoolean macro -->
    #set ($value = $!item.getStringProperty($property))
    #if( "$!value" == "" )
    (No value given)
    #elseif ($value)
    Yes
    #else
    No
    #end
<!-- END Generated by the showBoolean macro -->
#end

#macro(showString $property $item)
<!-- BEGIN Generated by the showString macro -->
    #set ($value = $!item.getBooleanProperty($property))
    #if( "$!value" == "" )
    (No value given)
    #elseif ($value)
    $value
    #else
    NAN
    #end
<!-- END Generated by the showString macro -->
#end

#macro(showInteger $property $item)
<!-- BEGIN Generated by the showInteger macro -->
    #set ($value = $!item.getIntegerProperty($property))
    #if( "$!value" == "" )
    (No value given)
    #elseif ($value)
    $value
    #else
    NAN
    #end
<!-- END Generated by the showInteger macro -->
#end

#macro(showDate $property $item)
<!-- BEGIN Generated by the showDate macro -->
    #set ($value = $!item.getDate($property)) 
    #if( "$!value" == "" )
    (No value given)
    #elseif ($value)
    $value
    #else
    NAN
    #end
<!-- END Generated by the showDate macro -->
#end

